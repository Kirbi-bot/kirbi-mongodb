  "xo": {
    "rules": {
      "linebreak-style": "off",
      "no-prototype-builtins": "warn",
      "radix": "warn",
      "guard-for-in": "warn",
      "import/no-unresolved": "off",
      "camelcase": "off",
      "no-negated-condition": "warn",
      "object-curly-spacing": [
        "error",
        "always",
        {
          "arraysInObjects": false
        }
      ]
    }
  },
  "scripts": {
    "test": "xo"
